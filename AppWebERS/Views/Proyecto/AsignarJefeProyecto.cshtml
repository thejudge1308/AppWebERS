
@{
                /**/

                ViewBag.Title = "Asignar Jefe Proyecto";

}
@using (Html.BeginForm("AsignarJefeProyecto", "Proyecto"))
{
    @Html.AntiForgeryToken()
<div class="form-horizontal">
    <h2>Asignar Jefe Proyecto</h2>
    <h4>Seleccione Proyecto</h4>
    <div>
        @Html.DropDownList("DropDownListJefe", (List<SelectListItem>)ViewBag.MiListadoProyectos)

        <h4>Seleccion Jefe de Proyecto</h4>
        @Html.DropDownList("DropDownListUsuarios", (List<SelectListItem>)ViewBag.MiListadoUsuarios)
    </div>
    <div><h6>@ViewBag.Message</h6></div>
    <div class="form-group">
        <div class="col-md-offset-2 col-md-9">
            <input type="submit" value="Crear Proyecto" class="btn btn-default" />
        </div>
    </div>



</div>
        }
        @section Scripts {
            @Scripts.Render("~/bundles/jqueryval")
        }
